@using System.Linq
@inherits ApiGenerator.CodeTemplatePage<List<string>>
@{
	var description = Model.Count == 1 ? Model.First() : string.Join($"{Environment.NewLine}\t\t", Model.Select(d => "/// " + d));
    if (Model.Count == 1)
    {
<text>      ///<summary>@Raw(description)</summary></text>
    }
    else
    {
<text>      ///<summary>
        @Raw(description)
        ///</summary>
</text>
    }
}
